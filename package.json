{
	"name": "electron-crash-report-server",
	"description": "Crash report server for Electron applications",
	"version": "1.2.0",
	"license": "MIT",
	"author": "John Muhl <johnmuhl@gmail.com>",
	"homepage": "https://johnmuhl.github.io/electron-crash-report-server",
	"repository": "github:johnmuhl/electron-crash-report-server",
	"bugs": "https://github.com/johnmuhl/electron-crash-report-server/issues",
	"scripts": {
		"build": "rollup -c",
		"start": "node -r esm server.js",
		"watch": "nodemon",
		"fmt": "prettier --loglevel=warn --write '**/*' && eslint . --ext=js,svelte --fix",
		"lint": "prettier --list-different '**/*' && eslint . --ext=js,svelte",
		"heroku-postbuild": "yarn build"
	},
	"engines": {
		"node": "10.x",
		"yarn": "1.x"
	},
	"dependencies": {
		"boom": "7.3.0",
		"dotenv": "6.2.0",
		"hapi": "17.8.1",
		"hapi-auth-basic": "5.0.0",
		"inert": "5.1.2",
		"massive": "5.7.2",
		"minidump": "0.15.0",
		"pg.js": "4.1.1",
		"pretty-ms": "4.0.0",
		"svelte": "alpha",
		"vision": "5.4.4"
	},
	"devDependencies": {
		"@ampproject/rollup-plugin-closure-compiler": "0.8.5",
		"eslint": "5.11.1",
		"eslint-config-airbnb-base": "13.1.0",
		"eslint-config-oz": "github:johnmuhl/eslint-config-oz#v5.1.0",
		"eslint-config-prettier": "3.3.0",
		"eslint-import-resolver-node": "0.3.2",
		"eslint-plugin-html": "5.0.0",
		"eslint-plugin-import": "2.14.0",
		"eslint-plugin-unicorn": "6.0.1",
		"esm": "3.0.84",
		"nodemon": "1.18.9",
		"prettier": "1.15.3",
		"rollup": "1.0.0",
		"rollup-plugin-commonjs": "9.2.0",
		"rollup-plugin-filesize": "5.0.1",
		"rollup-plugin-node-resolve": "4.0.0",
		"rollup-plugin-svelte": "5.0.0"
	},
	"private": true
}
