{
	"name": "Electron crash report server",
	"description": "Crash report server for Electron applications",
	"repository": "https://github.com/johnmuhl/electron-crash-report-server",
	"keywords": [
		"electron",
		"breakpad",
		"breakpad-server",
		"crash",
		"crash-reports",
		"hapi",
		"server"
	],
	"addons": [
		{
			"plan": "heroku-postgresql",
			"options": {
				"version": "11"
			}
		}
	],
	"env": {
		"AUTH_USER": "crash",
		"AUTH_PASS": "electron"
	}
}
